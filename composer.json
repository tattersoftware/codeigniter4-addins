		"tatter/alerts": "^3.0",
		"tatter/assets": "^3.0",
		"tatter/audits": "^1.0",
		"tatter/outbox": "^2.0",
		"tatter/permits": "^3.0",
		"tatter/preferences": "^1.0",
		"tatter/relations": "^2.0",
		"tatter/themes": "^2.0",
