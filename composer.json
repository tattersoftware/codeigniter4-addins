{
	"name": "tatter/addins",
	"description": "Meta-package of Tatter modules for CodeIgniter 4",
	"keywords": [
		"codeigniter",
		"codeigniter4",
		"tattersoftware",
		"tatter",
		"suite",
		"addins",
		"modules",
		"libraries",
		"packages"
	],
	"homepage": "https://github.com/tattersoftware/codeigniter4-addins",
	"license": "MIT",
	"authors": [
		{
			"name": "Matthew Gatner",
			"email": "mgatner@tattersoftware.com",
			"homepage": "https://tattersoftware.com",
			"role": "Developer"
		}
	],
	"require": {
		"php" : "^7.4 || ^8.0",
